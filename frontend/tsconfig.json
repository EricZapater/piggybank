{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/screens/*": ["src/screens/*"],
      "@/components/*": ["src/components/*"],
      "@/api/*": ["src/api/*"],
      "@/hooks/*": ["src/hooks/*"],
      "@/context/*": ["src/context/*"],
      "@/navigation/*": ["src/navigation/*"],
      "@/theme/*": ["src/theme/*"]
    }
  }
}
