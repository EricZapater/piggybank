{
  "expo": {
    "name": "Piggybank",
    "slug": "piggybank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "exp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zenithsoftwareproductions.piggybank"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zenithsoftwareproductions.piggybank"
    },
    "web": {
      "bundler": "metro",
      "devServer": {
        "port": 8080
      }
    },
    "extra": {
      "apiUrl": "http://localhost:8080"
    }
  }
}
